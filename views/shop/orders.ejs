<%-include('../includes/head.ejs')%>

</head>
<body>
  <%- include('../includes/navigation')%>
    <main>
        <% if(orders.length <= 0) { %> 

        <h1>Nothing is there</h1>
        <% } else{ %>
            <ul>
              <% orders.forEach(order=>{ %>
                <li>
                  <h1>#<%= order._id %></h1>
                  <ul>
                     <% order.items.forEach(product=>{ %>
                        <li><%= product.title %> (<%= product.quantity %>)</li>
                     <% }); %>
                  </ul>
                </li>
              <% }); %>
             
            </ul>
          <% } %>
    </main>
  
<%-include('../includes/end.ejs')%>